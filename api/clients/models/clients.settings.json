{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "clients",
  "info": {
    "name": "clients"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "phoneMobile": {
      "type": "string"
    },
    "phoneHome": {
      "type": "string"
    },
    "activityLog": {
      "type": "component",
      "repeatable": true,
      "component": "activity-log.activity-log"
    },
    "createdBy": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "createdClients"
    },
    "modifiedBy": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "modifiedClients"
    },
    "assignedTo": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "assignedClients",
      "dominant": true
    },
    "recordName": {
      "type": "string"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
