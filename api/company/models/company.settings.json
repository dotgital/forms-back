{
  "connection": "default",
  "collectionName": "companies",
  "info": {
    "name": "company"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "company"
    }
  }
}
