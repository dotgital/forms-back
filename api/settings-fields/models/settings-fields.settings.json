      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "required": {
      "type": "boolean"
    },
    "visible": {
      "type": "boolean"
    },
    "tableVisible": {
      "type": "boolean"
    },
    "tablePosition": {
      "type": "integer"
    },
    "dropdownOptions": {
      "type": "component",
      "repeatable": true,
      "component": "dropdown-options.dropdown-options"
    },
    "contentType": {
      "type": "string"
    },
    "column": {
      "type": "enumeration",
      "enum": [
        "right",
        "left",
        "hidden"
      ]
    },
    "recordName": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Dropdown",
        "Text",
        "Number",
        "Date"
      ]
    },
    "defaultValue": {
      "type": "string"
    },
