{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "createdClients": {
      "collection": "clients",
      "via": "createdBy"
    },
    "modifiedClients": {
      "collection": "clients",
      "via": "modifiedBy"
    },
    "assignedClients": {
      "collection": "clients",
      "via": "assignedTo"
    },
    "company": {
      "model": "company",
      "via": "users"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "custom_permission": {
      "model": "custom-permissions",
      "via": "user"
    },
    "userPreferences": {
      "type": "component",
      "repeatable": false,
      "component": "user-preferences.user-preferences"
    },
    "recordName": {
      "type": "string"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "phoneNumber": {
      "type": "string"
    },
    "office": {
      "type": "string"
    },
    "jobPosition": {
      "type": "string"
    },
    "userPermissions": {
      "type": "component",
      "repeatable": false,
      "component": "permissions.permissions"
    }
  }
}
